        "base64-arraybuffer": "^1.0.2",
        "eslint": "^10.0.1",
        "eslint-config-prettier": "^10.0.2",
        "eslint-plugin-svelte": "^3.0.2",
        "prettier": "^3.1.0",
        "prettier-plugin-svelte": "^3.2.6",
        "svelte": "^5.53.6",
        "svelte-check": "^4.3.4",
        "typescript": "^5.5.0",
        "vite": "^7.2.4",
